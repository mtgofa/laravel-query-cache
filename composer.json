{
    "name": "mtgofa/laravel-query-cache-relationship",
    "description": "Laravel database cache package and missing relationship events.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "elequent",
        "cache",
        "relationship",
        "relations",
        "events"
    ],
    "authors": [
        {
            "name": "Mohamed Tarek Gofa",
            "email": "gofa_25@hotmail.com"
        },
        {
            "name": "Musa Kurt",
