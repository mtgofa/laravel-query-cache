        "illuminate/container": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/events": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3|^9.0|^9.3",
        "orchestra/testbench": "^4.0|^5.0|^6.0|dev-remove-nice-to-have-deps"
    },
    "autoload": {
        "psr-4": {
            "MTGofa\\QueryCache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MTGofa\\QueryCache\\Providers\\QueryCacheServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MTGofa\\QueryCache\\Tests\\": "./tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
